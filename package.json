    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "babel-preset-next": "^1.4.0",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.4.0",
    "react-lottie": "^1.2.3",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6"
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
